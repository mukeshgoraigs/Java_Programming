	static int[] countAlpha(String str) {
	int a[]=new int[26];

for (int i = 0; i < str.length(); i++) {
	if(str.charAt(i)>=65&&str.charAt(i)<=90) {
		a[str.charAt(i)-65]++;
	}
	else if(str.charAt(i)>=97&&str.charAt(i)<=122) {
		a[str.charAt(i)-97]++;
	}
	return a;
}

