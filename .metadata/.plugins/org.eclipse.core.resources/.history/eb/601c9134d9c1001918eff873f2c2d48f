package array;

public class SelectionSort {
	public static void main(String[] args) {
		int[] a= {36,5,56,78,6,98,54,12,43};
		int min, temp=0;
		for (int i = 0; i < a.length; i++) {
			min=i;
			for (int j = i+1; j < a.length; j++) {
				if(a[j]<a[min]) {
					a[j]=min;
				}
			}
			temp=a[i];
			a[i]=a[min];
			a[min]=temp;
		}
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
	}
}
