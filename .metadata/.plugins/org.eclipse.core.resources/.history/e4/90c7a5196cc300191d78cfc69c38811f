package array;

public class DuplicateStringDelete {
	public static void main(String[] args) {
		String a="mukeshgoraiaaamukesh";
		
		for (int i = 0; i <a.length(); i++) {
			for (int j = i+1; j <a.length(); j++) {
				if(a.charAt(i)!=a.charAt(j)) {
					System.out.print(a.charAt(i));
					break;
				}
				
				
			}
			System.out.print(a.charAt(i));
		
		}
	}
}
